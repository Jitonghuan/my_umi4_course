    "@umijs/hooks": "~1.9.3",
    "ahooks": "~2.10.8",
    "classnames": "~2.3.1",
    "codemirror": "~5.62.0",
    "echarts": "^5.1.2",
    "events": "~3.3.0",
    "lodash": "~4.17.21",
