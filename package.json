{
  "name": "fe-matrix",
  "description": "fe-matrix",
  "version": "2.0.0",
  "scripts": {
    "inject": "node script/config-inject.js",
    "umi": "cross-env MOCK=none umi dev",
    "dev": "fnpm i && npm run inject -- dev && npm run umi",
    "start": "fnpm i && npm run inject -- dev && npm run umi",
    "umi:mock": "umi dev",
    "start:mock": "fnpm i && npm run inject -- dev && npm run umi:mock",
    "umi:build": "umi build",
    "build": "fnpm i && npm run inject && umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.cfuture.shop/fe-data-app/fe-matrix.git"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-table": "~2.34.4",
    "@cffe/fe-backend-component": "0.1.49",
    "@cffe/fe-datav-components": "0.1.8",
    "@cffe/fe-tpl": "0.2.7",
    "@cffe/pc-editor-table": "^0.1.3",
    "@cffe/vc-b-card-layout": "1.0.8",
    "@cffe/vc-custom-icon": "1.0.1",
    "@cffe/vc-error-page": "1.0.1",
    "@cffe/vc-form": "1.0.7",
    "@cffe/vc-hulk-form": "1.0.11",
    "@cffe/vc-hulk-table": "1.0.12",
    "@cffe/vc-layout": "1.0.38",
    "@cffe/vc-loading": "1.0.2",
    "@cffe/vc-request": "1.0.11",
    "@umijs/hooks": "~1.9.3",
    "ahooks": "~2.10.0",
    "classnames": "~2.3.1",
    "codemirror": "~5.61.1",
    "dayjs": "1.10.4",
    "events": "~3.3.0",
    "lodash": "~4.17.21",
    "moment": "~2.29.1",
    "qs": "~6.10.1",
    "react-codemirror2": "~7.2.1",
    "relaxed-json": "~1.0.3",
    "umi": "~3.3.7",
    "white-react-use": "~2.4.0"
  },
  "devDependencies": {
    "@hbos/umi-plugin-theme-hbos": "^0.0.14",
    "@irim/cli-base": "^1.1.9",
    "@types/codemirror": "^5.60.0",
    "@types/events": "^3.0.0",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/relaxed-json": "^1.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.7",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "typescript-require": "^0.3.0",
    "yorkie": "^2.0.0"
  },
  "__npminstall_done": false
}
