{
  "name": "fe-matrix",
  "description": "fe-matrix",
  "version": "2.0.0",
  "scripts": {
    "umi": "cross-env MOCK=none umi dev",
    "dev": "cross-env NODE_ENV=development MOCK=none umi dev",
    "start": "fnpm i && npm run dev",
    "umi:mock": "umi dev",
    "build": "cross-env BUILD_ENV=test umi build",
    "build:test": "cross-env BUILD_ENV=test umi build",
    "build:prod": "cross-env BUILD_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.cfuture.shop/fe-data-app/fe-matrix.git"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
