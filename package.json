{
  "name": "fe-matrix",
  "description": "fe-matrix",
  "version": "1.0.2",
  "scripts": {
    "inject": "node script/config-inject.js",
    "umi": "cross-env MOCK=none umi dev",
    "start": "fnpm i && npm run inject -- dev && npm run umi",
    "umi:mock": "umi dev",
    "start:mock": "fnpm i && npm run inject -- dev && npm run umi:mock",
    "umi:build": "umi build",
    "build": "fnpm i && npm run inject && umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "page": "node ./script/add-page.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.cfuture.shop/fe-data-app/fe-matrix.git"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-table": "^2.34.4",
    "@cffe/fe-backend-component": "0.1.49",
    "@cffe/fe-datav-components": "0.1.8",
    "@cffe/fe-tpl": "0.2.7",
    "@cffe/vc-b-card-layout": "^1.0.8",
    "@cffe/vc-custom-icon": "1.0.1",
    "@cffe/vc-error-page": "^1.0.1",
    "@cffe/vc-form": "1.0.7",
    "@cffe/vc-hulk-form": "1.0.11",
    "@cffe/vc-hulk-table": "1.0.7",
    "@cffe/vc-layout": "1.0.15",
    "@cffe/vc-loading": "1.0.2",
    "@cffe/vc-request": "1.0.10",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "ahooks": "^2.10.0",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "umi": "^3.3.7",
    "white-react-use": "^2.4.0"
  },
  "devDependencies": {
    "@hbos/umi-plugin-theme-hbos": "^0.0.14",
    "@irim/cli-base": "^1.1.9",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.3.7",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "typescript-require": "^0.3.0",
    "yorkie": "^2.0.0"
  },
  "__npminstall_done": false
}
